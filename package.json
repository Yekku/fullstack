{
	"name": "fullstack",
	"version": "1.0.0",
	"description": "fullstack app with angular and express",
	"main": "index.js",
	"engines": {
		"node": "9.9.0",
		"npm": "5.6.0"
	},
	"scripts": {
		"start": "NODE_ENV=production node index.js",
		"server": "NODE_ENV=development nodemon index.js",
		"client-install": "npm install --prefix client",
		"client": "npm run start --prefix client",
		"dev": "concurrently \"npm run server\" \"npm run client\"",
		"heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run client-install && npm run build --prefix client",
		"test": "NODE_ENV=test jest --verbose",
		"lint": "eslint ."
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Yekku/fullstack.git"
	},
	"keywords": [
		"express",
		"angular"
	],
	"author": "Yekku",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Yekku/fullstack/issues"
	},
	"homepage": "https://github.com/Yekku/fullstack#readme",
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.18.3",
		"cors": "^2.8.4",
		"express": "^4.16.3",
		"jsonwebtoken": "^8.2.1",
		"moment": "^2.22.1",
		"mongoose": "^5.2.8",
		"morgan": "^1.9.0",
		"multer": "^1.3.0",
		"passport": "^0.4.0",
		"passport-jwt": "^4.0.0"
	},
	"devDependencies": {
		"concurrently": "^3.5.1",
		"nodemon": "^1.17.3",
		"webpack": "^4.28.3",
		"webpack-dev-server": ">=3.1.11"
	}
}
