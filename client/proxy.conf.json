    "secure": false,
    "changeOrigin": true,
    "logLevel": "debug",
    "pathRewrite": {
      "^/api": "http://localhost:5001/api"
    }
  },
  "/uploads/*": {
    "target": "http://localhost:5001",
    "secure": false,
    "logLevel": "debug",
    "pathRewrite": {
      "^/api": "http://localhost:5001/uploads"
    }
  }
}
